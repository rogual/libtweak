#!/usr/bin/env python3

values = []

with open("Tweakfile", "rt") as tf:
    for line in tf:
        line = line.strip()
        if line:
            line = line.split(' ')
            cmd, name, *args = line
            if cmd == 'set':
                value, = map(float, args)
                values.append((name, value))

print('''
namespace tweak_baked
{
%s
}
''' % '\n'.join(
    '  const float %s = %s;' % (
        name, value
    ) for name, value in values
))
